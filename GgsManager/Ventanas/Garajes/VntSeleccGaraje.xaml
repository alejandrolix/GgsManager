<Window x:Class="VntSeleccGaraje"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GgsManager"
        mc:Ignorable="d"
        Title="Seleccionar Garaje" Height="275" Width="389"
        ResizeMode="NoResize" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen">

    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Garaje" Margin="0,0,20,0" FontSize="15" />
            <ComboBox Name="GarajesCmb" IsEditable="True" Width="110" Height="20" ItemsSource="{Binding}" />
        </StackPanel>

        <Button Name="AceptarBtn" Content="Aceptar" Width="68" FontSize="15" Margin="0,54,0,0" Click="AceptarBtn_Click" />
    </StackPanel>
</Window>
