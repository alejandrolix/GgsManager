<UserControl x:Class="VntCambiarIVA"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GgsManager"
        mc:Ignorable="d"
        Background="#f0f0f0">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <StackPanel Orientation="Vertical">
                <TextBlock Text="Nuevo I.V.A." FontSize="16" />
                <TextBox Name="NuevoIVATxt" Width="150" Height="25" FontSize="15" Margin="0,22,0,0" />
                <Button Name="GuardarIVABtn" Content="Aceptar" FontSize="15" Width="83" Height="26" Margin="0,50,0,0" Click="GuardarIVABtn_Click" />
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
