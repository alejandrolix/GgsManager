<Window x:Class="VntCambiarIVA"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GgsManager"
        mc:Ignorable="d"
        Title="Cambiar I.V.A." Height="310" Width="464" Background="#f0f0f0" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
        Icon="/GgsManager;component/Logo.ico">

    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <StackPanel Orientation="Vertical">
                <TextBlock Text="Nuevo I.V.A." FontSize="18" />
                <TextBox Name="NuevoIVATxt" Width="160" Height="26" FontSize="15" Margin="0,24,0,0" />
                <Button Name="GuardarIVABtn" Content="Guardar" FontSize="18" Width="90" Height="34" Margin="0,60,0,0" Click="GuardarIVABtn_Click" />
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
